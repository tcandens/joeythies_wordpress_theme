{% extends "base.twig" %}

{% block content %}
  <div class="random-x luna-boot-scoot-container">
    <div class="luna-boot-scoot"></div>
  </div>
  <main class="o-col o-center c-buffered" role="main">
    {% if intro %}
      <header class="o-row c-insetbox">
        <section class="c-insetbox__inner">
          {% include "logo.twig" %}
          {{ intro.content }}
        </section>
      </header>
    {% endif %}
    {% for post in posts %}
      {% include post.post_type~"-entry.twig" %}
    {% endfor %}
  </main>
{% endblock %}
